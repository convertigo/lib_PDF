<!--      <Sequence JS : fill>                                                                                                                            --><step classname="com.twinsoft.convertigo.beans.steps.SimpleStep" newPriority="1532703526538" priority="1532703526538"><property name="comment"><java.lang.String value="&#13;&#10;"/></property><property name="expression"><java.lang.String value="/**&#13;&#10; * Based on PDFBox 2.0.11 (pdfbox-app-2.0.11.jar) This JAR must be installed on the Convertigo ClassPath.&#13;&#10; * This is automatically the case for Convertigo 7.6&#13;&#10; */&#13;&#10;&#13;&#10;var proj = context.getProjectDirectory();&#13;&#10;var wks = com.twinsoft.convertigo.engine.Engine.PROJECTS_PATH +&quot;/&quot;;&#13;&#10;var tplDir = &quot;pdf_Templates/&quot;;&#13;&#10;var srcFile = wks + tplDir + tplPDF;&#13;&#10;var dstFile = wks + tplDir + tgtPDF;&#13;&#10;var pdf = null;&#13;&#10;var acroForm = null;&#13;&#10;&#13;&#10;log.warning(&quot;Proj:&quot; + proj);&#13;&#10;log.warning(&quot;Wkrs:&quot; + wks);&#13;&#10;log.warning(&quot;Source PDF template file:&quot; + srcFile);&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;function getFieldArea(field) {&#13;&#10;    fieldDict = field.getCOSObject();&#13;&#10;    fieldAreaArray = fieldDict.getDictionaryObject(org.apache.pdfbox.cos.COSName.RECT);&#13;&#10;    return new org.apache.pdfbox.pdmodel.common.PDRectangle(fieldAreaArray);&#13;&#10;}&#13;&#10;&#13;&#10;&#13;&#10;function handleImages(field, value, doc) {&#13;&#10;    if( field != null ) {&#13;&#10;        widgets = field.getWidgets();&#13;&#10;        if (widgets != null &amp;&amp; widgets.size() &gt; 0) {&#13;&#10;            annotationWidget = widgets.get(0); // just need one widget&#13;&#10;            imagePath = wks + tplDir + value;&#13;&#10;            image = new java.io.File(imagePath);&#13;&#10;            log.warning(&quot;Got Imagefile &quot; + image);&#13;&#10;            if (image.exists()) {&#13;&#10;                pdImageXObject = org.apache.pdfbox.pdmodel.graphics.image.PDImageXObject.createFromFile(imagePath, doc);&#13;&#10;                imageScaleRatio = pdImageXObject.getHeight() / pdImageXObject.getWidth();&#13;&#10;                buttonPosition = getFieldArea(field);&#13;&#10;                height = buttonPosition.getHeight();&#13;&#10;                width = height / imageScaleRatio;&#13;&#10;                x = buttonPosition.getLowerLeftX();&#13;&#10;                y = buttonPosition.getLowerLeftY();&#13;&#10;                log.warning(&quot;Image coords (x, y, w, h): &quot; + x +&quot;,&quot;+ y +&quot;,&quot;+ width +&quot;,&quot;+ height +&quot;,&quot; );&#13;&#10;&#13;&#10;                pdAppearanceStream = new org.apache.pdfbox.pdmodel.interactive.annotation.PDAppearanceStream(pdf);&#13;&#10;                pdAppearanceStream.setResources(new org.apache.pdfbox.pdmodel.PDResources());&#13;&#10;                pdPageContentStream = new org.apache.pdfbox.pdmodel.PDPageContentStream(pdf, pdAppearanceStream);&#13;&#10;                pdPageContentStream.drawImage(pdImageXObject, x, y, width, height);&#13;&#10;                pdAppearanceStream.setBBox(new org.apache.pdfbox.pdmodel.common.PDRectangle(x, y, width, height));&#13;&#10;                pdAppearanceDictionary = annotationWidget.getAppearance();&#13;&#10;                if (pdAppearanceDictionary == null) {&#13;&#10;                    pdAppearanceDictionary = new PDAppearanceDictionary();&#13;&#10;                    annotationWidget.setAppearance(pdAppearanceDictionary);&#13;&#10;                }&#13;&#10;                pdAppearanceDictionary.setNormalAppearance(pdAppearanceStream);&#13;&#10;                pdPageContentStream.close();&#13;&#10;                log.warning(&quot;Image is inserted...&quot;);&#13;&#10;            } else {&#13;&#10;                log.error(&quot;File: &quot; + imagePath + &quot; is not found&quot;);&#13;&#10;            }&#13;&#10;        } else {&#13;&#10;            log.error(&quot;Missconfiguration of placeholder: '&quot; + fieldName + &quot;' - no widgets(actions) found&quot;);&#13;&#10;        }&#13;&#10;    }&#13;&#10;}&#13;&#10;&#13;&#10;&#13;&#10;function replaceField(name, data, doc){&#13;&#10;&#9;var field = acroForm.getField( name );&#13;&#10;    if( field != null ) {&#13;&#10;    &#9;if(field.isReadOnly())&#13;&#10;    &#9;&#9;field.setReadOnly(false);&#13;&#10;    &#9;&#9;&#13;&#10;    &#9;switch (&quot;&quot;+field.getClass().getSimpleName()) {&#13;&#10;    &#9;&#9;case &quot;PDTextField&quot;:&#13;&#10;//    &#9;&#9;&#9;log.warning(&quot;TEXT Field: &quot; + name + &quot;=&quot; + data);&#13;&#10;    &#9;&#9;&#9;field.setValue(data);&#13;&#10;    &#9;&#9;&#9;break;&#13;&#10;    &#9;&#9;case &quot;PDCheckBox&quot;:&#13;&#10;//    &#9;&#9;&#9;log.warning(&quot;CHECKBOX Field: &quot;+ name + &quot;=&quot; + data);&#13;&#10;    &#9;&#9;&#9;if (data == &quot;true&quot;)&#13;&#10;&#9;    &#9;&#9;&#9;field.check();&#13;&#10;&#9;    &#9;&#9;else&#13;&#10;&#9;    &#9;&#9;&#9;field.unCheck();&#13;&#10;    &#9;&#9;&#9;break;&#13;&#10;    &#9;&#9;&#9;&#13;&#10;    &#9;&#9;case &quot;PDPushButton&quot;:&#13;&#10;    &#9;&#9;&#9;handleImages(field, data, doc);&#13;&#10;    &#9;&#9;&#9;break;&#13;&#10;    &#9;&#9;&#9;&#13;&#10;    &#9;&#9;default:&#13;&#10;    &#9;&#9;&#9;log.warning(&quot;UNKNOWN Field : &quot; + field.getClass().getSimpleName());&#13;&#10;    &#9;&#9;break;&#13;&#10;    &#9;}&#13;&#10;    &#9;&#13;&#10;        field.setReadOnly(true);&#13;&#10;    }&#13;&#10;    else {&#13;&#10;        log.warning( &quot;No field found with name:&quot; + name );&#13;&#10;    }&#13;&#10;}&#13;&#10;&#13;&#10;try {&#13;&#10;&#9;// Load PDF file and Form Catalog:&#13;&#10;&#9;var file = new java.io.File(srcFile);&#13;&#10;&#9;pdf = org.apache.pdfbox.pdmodel.PDDocument.load(file);&#13;&#10;&#9;var docCatalog = pdf.getDocumentCatalog();&#13;&#10;&#9;acroForm = docCatalog.getAcroForm();&#13;&#10;&#9;&#13;&#10;&#9;// Read replacement JSON string: